<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label fx:id="lblFetchedProductAreas" maxWidth="1.7976931348623157E308" text="Fetching Product Areas..." />
      <Separator prefWidth="200.0" />
      <GridPane hgap="5.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Product id:" />
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Product Name:" textAlignment="RIGHT" GridPane.rowIndex="1" />
            <Label fx:id="lblProductName" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" text="&lt;???&gt;" GridPane.columnIndex="1" GridPane.columnSpan="2147483647" GridPane.rowIndex="1" />
            <TextField fx:id="tvProductId" promptText="P-id..." GridPane.columnIndex="1" />
            <Button fx:id="btnFetchProductDto" alignment="CENTER" mnemonicParsing="false" text="Fetch" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
         </children>
      </GridPane>
      <Separator prefWidth="200.0" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0">
         <children>
            <ListView fx:id="lvProductProductAreas" prefHeight="200.0" prefWidth="200.0" />
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Button fx:id="btnAddProductProductArea" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Add..." />
                  <Button fx:id="btnRemoveProductProductArea" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Remove" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <Button fx:id="btnUpdateProductDto" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Update!" />
   </children>
</VBox>
